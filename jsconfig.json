{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@config": [
        "./src/config.js"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@api": [
        "./src/api/index.js"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@utils": [
        "./src/utils/index.js"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
      "@validation/*": [
        "./src/validation/*"
      ],
      "@dict/*": [
        "./src/dict/*"
      ],
      "@views/*": [
        "./src/views/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@vendor/*": [
        "./src/vendor/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}